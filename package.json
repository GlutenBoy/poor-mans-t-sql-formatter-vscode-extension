{
    "name": "poor-mans-t-sql-formatter",
    "displayName": "Poor Man's T-SQL Formatter",
    "description": "A Visual Studio Code extension for Poor Man's T-SQL Formatter",
    "version": "1.6.6",
    "publisher": "TaoKlerks",
    "engines": {
        "vscode": ">=1.0.0 <2.0.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.sayHello"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [{
            "command": "extension.sayHello",
            "title": "Hello World"
        }]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "eslint": "^4.6.1",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}